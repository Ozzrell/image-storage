{
	"name": "contributte/image-storage",
	"type": "library",
	"description": "Image storage for Nette framework",
	"keywords": [
		"image",
		"nette",
		"storage",
		"crop",
		"resize",
		"upload"
	],
	"license": [
		"MIT"
	],
	"support": {
		"issues": "https://github.com/contributte/image-storage/issues"
	},
	"authors": [
		{
			"name": "Pavel Janda",
			"homepage": "https://paveljanda.com"
		}
	],
	"require": {
		"php": ">=8.1",
		"nette/application": "^3.1.14",
		"nette/utils": "^4.0.3",
		"nette/di": "^3.1.8",
		"nette/http": "^3.2.3",
		"latte/latte": "^3.0.12"
	},
	"require-dev": {
    "contributte/qa": "^0.4",
    "contributte/tester": "^0.3",
    "contributte/phpstan": "^0.1",
		"tracy/tracy": "^2.10.5"
	},
  "autoload": {
    "psr-4": {
      "Contributte\\ImageStorage\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "3.0.x-dev"
    }
  }
}
